<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.foucrazy.dao.NivelesMapper">
    
	<select id="findByPK" resultType="com.foucrazy.model.Nivel" parameterType="int">
		select "idNivel",valor from niveles where "idNivel"=#{id}
	</select>
	
	<select id="getAll" resultType="com.foucrazy.model.Nivel">
		select "idNivel",valor from niveles
	</select>
	
</mapper>
